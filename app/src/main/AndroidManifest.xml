<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.android.tictactoe2">

    <application
        android:allowBackup="true"
        android:fullBackupContent="@xml/backup_descriptor"
        android:icon="@drawable/ttt2"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity
            android:name=".Splashscreen.SplashScreen"
            tools:ignore="InnerclassSeparator">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".BoardType.FiveBoardActivity"
            tools:ignore="InnerclassSeparator" />
        <activity
            android:name=".BoardType.ThreeBoardActivity"
            tools:ignore="InnerclassSeparator" />
        <activity
            android:name=".OptionsActivity.OptionMenu"
            tools:ignore="InnerclassSeparator" />
        <activity
            android:name=".OptionsActivity.AboutUs"
            tools:ignore="InnerclassSeparator" />
        <activity
            android:name=".OptionsActivity.InstructionActivity"
            tools:ignore="InnerclassSeparator" />
        <activity
            android:name=".OptionsActivity.PlayerMode"
            tools:ignore="InnerclassSeparator" />
        <activity
            android:name=".NameActivity.NameThreeBoard"
            tools:ignore="InnerclassSeparator" />
        <activity
            android:name=".OptionsActivity.AppsByNero"
            tools:ignore="InnerclassSeparator" />
        <activity
            android:name=".NameActivity.NameFiveBoard"
            tools:ignore="InnerclassSeparator" />
    </application>

</manifest>